<%- include("./partials/header.ejs") %>
<main>
    <% if(blogs.length>0){
            blogs.forEach(blog => { %>
    <div class="post">
        <h2><%= blog.title %></h2>
        <p><%= blog.snippet %></p>
    </div>
    <% });
        } else { %>
    <p>No blogs to display</p>
    <% } %>

</main>
<%- include("./partials/footer.ejs") %>